<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.chartdemo">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".activity.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".activity.LineChartActivity"></activity>
        <activity android:name=".activity.BarChartActivity"></activity>
        <activity android:name=".activity.HorizontalBarChartActivity"></activity>
        <activity android:name=".activity.CombinedChartActivity"></activity>
        <activity android:name=".activity.PieChartActivity"></activity>
        <activity android:name=".activity.ScatterChartActivity"></activity>
        <activity android:name=".activity.CandleStickChartActivity"></activity>
        <activity android:name=".activity.BubbleChartActivity"></activity>
        <activity android:name=".activity.RadarChartActivity"></activity>
    </application>

</manifest>